<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="fuelVbox" alignment="CENTER" prefHeight="434.0" prefWidth="600.0" spacing="20.0" style="-fx-background-color: #3aafa9;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Gruppe24.OSLOMET.Controllers.SetFuel_Controller">
   <children>
      <Label text="Choose the type of fuel" textFill="#def2f1">
         <font>
            <Font name="System Bold" size="30.0" />
         </font>
      </Label>
      <ScrollPane fx:id="scrollPane" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #3aafa9;">
         <content>
            <StackPane prefHeight="190.0" prefWidth="556.0">
               <children>
                  <VBox fx:id="vboxFuel" alignment="CENTER" prefHeight="195.0" prefWidth="558.0" />
               </children>
            </StackPane>
         </content>
      </ScrollPane>
      <Button contentDisplay="CENTER" graphicTextGap="0.0" onAction="#btnToWheels" style="-fx-background-color: #17252a;" text="Next page" textAlignment="CENTER" textFill="#def2f1" />
      <Button contentDisplay="CENTER" graphicTextGap="0.0" onAction="#backBtn" prefHeight="25.0" prefWidth="71.0" style="-fx-background-color: #17252a;" text="Back" textAlignment="CENTER" textFill="#def2f1" />
      <Label fx:id="lblErrorFuel" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
